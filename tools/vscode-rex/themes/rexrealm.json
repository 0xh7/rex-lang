{
  "name": "rex realm   ",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "name": "Types",
      "scope": ["support.type", "entity.name.type"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "entity.name.function.call",
        "support.function",
        "support.function.*"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Modules and namespaces",
      "scope": ["support.module", "entity.name.namespace"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable.other", "variable.other.member"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Language constants and enum variants",
      "scope": ["constant.language", "support.constant.enum-variant"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#D4D4D4"
      }
    }
  ]
}
